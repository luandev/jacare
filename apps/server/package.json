{
  "name": "@crocdesk/server",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "pretest": "npm run build -w @crocdesk/shared",
    "test": "vitest run",
    "package": "pkg dist/index.js --targets node20-win-x64,node20-macos-x64,node20-linux-x64 --output-path ../../release/server/jacare-server"
  },
  "dependencies": {
    "@crocdesk/shared": "*",
    "better-sqlite3": "^9.4.1",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "p-queue": "^8.0.1",
    "unzipper": "^0.10.11"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.9",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "pkg": "^5.8.1",
    "tsx": "^4.7.0",
    "vitest": "^2.0.5"
  }
}
